rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    match /allowedEmails/{docId} {
      allow read;  // âœ… Allows read access to `allowedEmails` collection
    }
    
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2025, 5, 1);
    }
    
  }
}
